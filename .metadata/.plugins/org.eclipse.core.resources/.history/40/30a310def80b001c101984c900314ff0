package Methodoverriding;

public interface Demo {
void display();
void view();
 //int arr();
}
class Test1 implements Demo{
	 public void display() {
		 int x=10;
		System.out.println("Abstract method display : From interface demo"+x);
	}
	public void view() {
		System.out.println("Abstract methdo display :");
	}
}
