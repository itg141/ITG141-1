package project;

import java.util.Scanner;

public class number {

	public static void main(String[] args) {
		int a,b=0,c;
		Scanner s=new Scanner(System.in);
		System.out.println("enter a number:");
		a=s.nextInt();
		if(a<50) {
			c=a+b;
			System.out.println(c);
		}
		else {
			System.out.println("greater than ");
		}
		
	}

}
