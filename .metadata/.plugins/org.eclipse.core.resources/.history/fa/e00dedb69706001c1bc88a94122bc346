//Write a program to convert the Time in hours to its seconds Equivalent (seconds = 3600 * hours)
package neww;
class Hts{
	int s;
	int convert(int h) {
		return(s=3600*h);
	}
}
public class P37 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Hts p =new Hts();
		System.out.println(p.convert(2);
		
	}

}
