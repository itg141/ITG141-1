package basicquestion;
////Find big digit in a number 
public class BigDigit {
       
	public static void main(String[] args) {
		int num=10657,temp=0, max=0;
 	   for(;num>0;) {
 		   temp=num;
 		   int r =num%10;
 		   if(max<r)
 		    num=num/10;
 		   }
    
	}

}
//// Questionable outputs: Reverify