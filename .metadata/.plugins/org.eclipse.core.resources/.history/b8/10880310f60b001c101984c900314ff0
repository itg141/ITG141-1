package Methodoverriding;

abstract class Main{
	abstract void fun();
	abstract void display();
}
class Derived extends Main {
void fun(){
	System.out.println("Abstract method 1 ");
}
void display() {
	System.out.println("Abstract method 2 ");
}
}
class Abstractclass {
public static void main(String args[])
{	Main b = new Derived();
	b.fun();
	b.display();
}
}
