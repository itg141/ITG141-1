package basicquestion;
//Program to whether all digits in a given number are equal or not?
public class EqualNum {

	public static void main(String[] args) {
		int num=111,rem=0,mun=0;
	 	   while(num>0) {
	 		    rem =num%10;
	 		    mun=rem;
	 		   if(mun==rem) 
	 			num=num/10;
	 		   }
	    System.out.println("Equal Number");
		}
	}


