package basicquestion;
// Print characters from a to z 
public class Chara {
	void Return(int a) {
		if(a<90 || a>65) {
		for(int i=a;i<=90;i++)
			System.out.println((char)i);
		}
		else if(a>97 && a<122) {
			for(int i=a;i<=122;i++)
				System.out.println((char)i);
		}
		else
			System.out.println("Special Character Entered : Not Found");
	}

	public static void main(String[] args) {
		Chara c= new Chara();
		c.Return(97);
	}

}
