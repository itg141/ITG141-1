package abraka;
class Cal{
	int a,b,c;
	int pro(int m, int n) {
		a=m;
		b=n;
		c=a*b;
		return c;
	}
	int sq(int d) {
		a=d;
		b=d;
		pro(a,b);
		return c;
	}
}

public class Mor {
public static void main(String[] args ) {
	Cal n= new Cal();
	System.out.println(n.pro(3, 6));
	System.out.println(n.sq(3));
}
}
