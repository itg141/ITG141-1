package BasicQuestion;

public class Series1 {
	void Return(int a) {
		double sum=0;
		double nm=0;
		while(a>0) {
			for(int i=1;i<=a;i++) {
			nm=(1/i);
			sum=sum+nm;
			}
			System.out.print(sum);
		}
		
	}
	public static void main(String[] args) {
	 Series1 ss = new Series1();
	 ss.Return(5);
	}

}
