package project;

import java.util.Scanner;

public class Neven {

	private static int i;

	public static void main(String[] args) {
		int n;
		Scanner s=new Scanner(System.in);
		System.out.println("the even numner:");
		n=s.nextInt();
		if(n%2==0)
			for(i=0;i<=n;i++)
				System.out.println(n);

	}

}
