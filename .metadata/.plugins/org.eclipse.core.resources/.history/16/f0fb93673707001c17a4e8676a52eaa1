package project;

import java.util.Scanner;

public class number {

	public static void main(String[] args) {
		int a,x,c;
		Scanner s=new Scanner(System.in);
		System.out.println(a+x);
		c=s.nextInt();
		

		
		if(c>50) 
			System.out.println("less than 50");
			else
				System.out.println("greater than 50");
			
		
		}
		
	}


