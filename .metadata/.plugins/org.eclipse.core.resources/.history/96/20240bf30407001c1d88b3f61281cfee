package project;

import java.util.Scanner;

public class Mul {
	private static int i;

	public static void main(String[] args) {
		int a,b;
		Scanner s=new Scanner(System.in);
		System.out.println("multiplication number:");
		a=s.nextInt();
		b=s.nextInt();
		for(i=0;i<=20;i++)
			System.out.println(a+*"i+"+b);
	}

}
