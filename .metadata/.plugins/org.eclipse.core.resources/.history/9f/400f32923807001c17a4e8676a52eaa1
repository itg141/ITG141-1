package project;

import java.util.Scanner;

public class table1 {
	

		public static void main(String[] args) {
			int a;
			Scanner s=new Scanner(System.in);
			System.out.println("multiplication number:");
			a=s.nextInt();
			
			for(int i = 1;i<=11;i++)
				System.out.println(a+"*"+i+"="+a*i);
		}

	}
	


