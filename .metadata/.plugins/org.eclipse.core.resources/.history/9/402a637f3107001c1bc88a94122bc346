package neww;
import java.util.*;
public class P41 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("Enter a number to check");
		Scanner sc= new Scanner(System.in); 
		int r= sc.nextInt();
		int n=242,c=0;
		int t=0;
		int l=0;
		while (l<3) {
			t=n%10;
			if(t==r) {
				System.out.println(c++);
			}
			else {
				System.out.println("Not found");
			}
		l++;
		}
}
}