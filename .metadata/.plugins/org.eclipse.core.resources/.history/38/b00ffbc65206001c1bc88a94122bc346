//Check whether the given number is divisible by both 3 and 5.
package neww;

public class FourthProgram {

	public static void main(String[] args) {
		int n=5;
		if ((n%3==0)&&(n%5==0)){
			System.out.println(n+" is divisible by both 3 and 5");
		}
		else {
			System.out.println(n+" is not is divisible by both 3 and 5");
		}
			

	}

}
