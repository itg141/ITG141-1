package neww;
import java.util.*;

public class P33 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("Enter a number");
		Scanner sc= new Scanner(System.in); 
		int k= sc.nextInt();
		int i1=0;
		int i2=1,i3;		
		for(int i=0;i<=k;i++) {
			i3=i1+i2;
			System.out.println(i3);
			i1=i2;
			i2=i3;
		}
	}

}
